{
  "name": "html-extraction",
  "version": "0.0.1",
  "author": "Jakub Kottnauer",
  "scripts": {
    "build": "webpack --config webpack.config.dev.js",
    "build:prod": "webpack --config webpack.config.prod.js",
    "clean": "rm -rf ./dist",
    "exp": "mkdir -p ./dist/exp && babel ./exp -d ./dist/exp && node ./dist/exp/index.js",
    "flow:setup": "yarn flow-typed install",
    "lint": "eslint ./src ./types ./exp",
    "lint:fix": "yarn lint --fix",
    "start": "open dist/index.html && yarn build",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.39",
    "@babel/core": "7.0.0-beta.39",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0-beta.39",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.39",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-flow": "^7.0.0-beta.39",
    "@babel/preset-stage-2": "^7.0.0-beta.39",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "chalk": "^2.3.0",
    "compression-webpack-plugin": "^1.1.6",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.64.0",
    "flow-typed": "^2.3.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "prettier": "^1.10.2",
    "puppeteer": "^1.0.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "npm-check-updates": "^2.14.0",
    "ramda": "^0.25.0",
    "whatwg-fetch": "^2.0.3"
  }
}
